{
  "name": "euventura/uazapi-sdk",
  "description": "SDK do UazApi para integração com APIs usando Saloon",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "whatsapp",
    "uatizap",
    "whatsapp api",
    "api",
    "whatsapp-api"
  ],
  "authors": [
    {
      "name": "euventura",
      "email": "euventura@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "saloonphp/saloon": "^3.0",
    "ext-curl": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "euventura\\UazapiSdk\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "euventura\\UazapiSdk\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}